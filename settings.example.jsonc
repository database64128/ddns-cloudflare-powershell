{
    // Leave empty to print to stdout.
    "LogFilePath": "/var/log/ddns-cloudflare-powershell.log",
    // The interval for periodic checks in seconds, 0 for a one-shot run.
    "Interval": 300,
    // Requires DNS edit permission. Create at https://dash.cloudflare.com/profile/api-tokens
    "OAuthToken": "<OAuthToken>",
    // Each domain name in your Cloudflare account has a unique zone ID.
    "ZoneID": "",
    "IPv6": {
        "Enabled": true,
        "Proxied": false,
        "Hostname": "example.com"
    },
    "IPv4": {
        "Enabled": true,
        "Proxied": false,
        "Hostname": "example.com",
        "AddressAPI": "https://api.ipify.org/"
    },
    // Whether to disable proxy for requests to Cloudflare API.
    // Note that requests to the address API are never proxied.
    "NoProxy": false
}
